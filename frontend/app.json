{
  "expo": {
    "name": "Frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/IstokWebBold.ttf",
            "./assets/fonts/IstokWebBoldItalic.ttf",
            "./assets/fonts/IstokWebItalic.ttf",
            "./assets/fonts/IstokWebRegular.ttf",
            "./assets/fonts/JomhuriaRegular.ttf"
          ]
        }
      ]
    ]
  }
}

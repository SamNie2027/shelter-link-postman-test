{
  "expo": {
    "name": "Frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samnie2027.frontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.samnie2027.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/IstokWebBold.ttf",
            "./assets/fonts/IstokWebBoldItalic.ttf",
            "./assets/fonts/IstokWebItalic.ttf",
            "./assets/fonts/IstokWebRegular.ttf",
            "./assets/fonts/JomhuriaRegular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf7b2ada-d5be-4f07-b74e-fd79806e1a81"
      }
    }
  }
}
